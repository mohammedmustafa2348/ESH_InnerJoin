<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.fblogin.PreferenceActivity"
    android:background="#eeebeb">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Select your preferences and interests."
        android:id="@+id/textTitle"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textAlignment="center"
        android:textColor="@color/com_facebook_button_background_color_pressed"
        android:textSize="20dp"
        android:layout_alignParentTop="true" />

    <GridLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#dad9d9"
        android:id="@+id/linearLayout2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textTitle"
        android:layout_marginTop="15dp">

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnProgramming"
            android:src="@drawable/programming"
            android:layout_marginTop="5dp"
            android:layout_row="0"
            android:layout_column="0" />
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnSinging"
            android:src="@drawable/singing"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="0"
            android:layout_column="1" />
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnVideoGames"
            android:src="@drawable/videogames"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="0"
            android:layout_column="2" />

        <ImageButton
            android:layout_width="129dp"
            android:layout_height="129dp"
            android:id="@+id/btnESH"
            android:src="@drawable/eastswedenhack"
            android:layout_marginTop="5dp"
            android:layout_row="1"
            android:layout_column="1" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnWriting"
            android:src="@drawable/writing"
            android:layout_gravity="right"
            android:layout_marginTop="5dp"
            android:layout_row="1"
            android:layout_column="0" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnMovies"
            android:src="@drawable/movies"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="1"
            android:layout_column="2" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnExercising"
            android:src="@drawable/exercising"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="2"
            android:layout_column="0" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnSports"
            android:src="@drawable/sports"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="2"
            android:layout_column="1" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnPhotography"
            android:src="@drawable/photography"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="2"
            android:layout_column="2" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnAnimals"
            android:src="@drawable/animals"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="3"
            android:layout_column="0" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnBoardGames"
            android:src="@drawable/boardgames"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="3"
            android:layout_column="1" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnPottery"
            android:src="@drawable/pottery"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="3"
            android:layout_column="2" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnLitterature"
            android:src="@drawable/litterature"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="4"
            android:layout_column="0" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnYoga"
            android:src="@drawable/yoga"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="4"
            android:layout_column="1" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnHunting"
            android:src="@drawable/hunting"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="4"
            android:layout_column="2" />
        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnFishing"
            android:src="@drawable/hunting"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="5"
            android:layout_column="2" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnDancing"
            android:src="@drawable/dancing"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="5"
            android:layout_column="1" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnDiy"
            android:src="@drawable/DIY"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="5"
            android:layout_column="1" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/btnCamping"
            android:src="@drawable/camping"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:layout_row="5"
            android:layout_column="1" />

    </GridLayout>

</RelativeLayout>
